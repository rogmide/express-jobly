{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "some"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                1000
            ]
        },
        "equity": {
            "type": "integer",
            "default": 0,
            "title": "The equity Schema",
            "examples": [
                0
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "somename"
            ]
        }
    },
    "examples": [{
        "title": "some",
        "salary": 1000,
        "equity": 0,
        "company_handle": "somename"
    }]
}